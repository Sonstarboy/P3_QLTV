<%= simple_form_for(@book) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">

    <%= f.input :isbn %>

    <%= f.input :name %>

    <%= f.input :author %>

    <%= f.input :publisher %>

    <%= f.input :year_published %>

    <%= f.input :description %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
